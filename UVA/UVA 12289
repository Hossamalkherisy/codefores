using namespace std;
long long arr[101], ar[100001];
int main(){
	long long x,h=0;
	cin >> x;
	
	string s,s1="one",s2="two",s3="three";
	for (int i = 0; i < x; i++){
		cin >> s;
		if (s.size() == s3.size())
		{
			cout << 3 << "\n";
			continue;
		}
		else
		{
			int m=0, n = 0;
			for (int h = 0; h < 3; h++){

				if (s[h] == s1[h])
					m++;
				if (s[h] == s2[h])
					n++;

			}
			if (m>n)
				cout << "1\n";
			else
				cout << "2\n";

		}

	}
}
