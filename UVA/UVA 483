#include <cstdio>
#include <set>
#include <complex>
#include <list>
using namespace std;
long long arr[101], ar[100001];
int main(){
	string s,ss;
	while (getline(cin, s)){

		for (int i = 0; i < s.size(); i++){
			if (s[i] != ' ')
				ss += s[i];
			else{
				reverse(ss.begin(), ss.end());
				cout << ss << " ";
				ss = "";
			}
		}
		reverse(ss.begin(), ss.end());
		cout << ss << endl;
		ss = "";
	}
}
