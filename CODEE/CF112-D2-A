using namespace std;
long long arr[101], ar[100001];
int main(){
	string h, m;
	cin >> h >> m;
	for (int i = 0; i < h.size(); i++){
		if (h[i] >= 65 && h[i] <= 96)
			h[i] += 32;
		if (m[i] >= 65 && m[i] <= 96)
			m[i] += 32;
	}
	cout << h.compare(m);
}
